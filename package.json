{
  "name": "dashboard",
  "description": "Personal dashboard",
  "version": "1.0.11",
  "author": "Darek Kay <hello@darekkay.com> (https://darekkay.com/)",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "i18next": "19.0.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-grid-layout": "0.17.1",
    "react-i18next": "11.0.1",
    "react-modal": "3.11.1",
    "react-redux": "7.1.1",
    "react-scripts": "3.2.0",
    "reakit": "1.0.0-beta.9",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "redux-starter-kit": "0.8.1",
    "selectorator": "4.0.3"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.2.5",
    "@storybook/addons": "5.2.5",
    "@storybook/react": "5.2.5",
    "@testing-library/react": "9.3.1",
    "@testing-library/react-hooks": "3.1.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.21",
    "@types/lodash": "4.14.144",
    "@types/moment": "2.13.0",
    "@types/node": "12.12.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-grid-layout": "0.16.7",
    "@types/react-modal": "3.10.0",
    "@types/react-redux": "7.1.5",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "node-sass": "4.13.0",
    "plop": "2.5.2",
    "prettier": "1.18.2",
    "storybook-addon-themes": "5.2.0",
    "tailwindcss": "1.1.3",
    "ts-node": "^8.4.1",
    "typescript": "3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://dashboard.darekkay.com",
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 75,
        "functions": 70,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/__stories__/**",
      "!**/react-app-env.d.ts",
      "!src/components/**/selectors.ts",
      "!src/widgets/**/properties.ts",
      "!src/widgets/**/configuration.tsx"
    ]
  },
  "license": "MIT",
  "main": "src/index.tsx",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/dashboard.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "ci": "npm run lint:check && npm run test",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint:check": "tsc && eslint src/**/*.{ts,tsx,js}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,js}",
    "scan-widgets": "ts-node -P internals/scripts/tsconfig.json internals/scripts/scan-widgets.ts && prettier --write src/widgets/list.ts",
    "start": "react-scripts start",
    "tailwind": "tailwind build internals/tailwind/tailwind.css -c internals/tailwind/config.js --no-autoprefixer -o src/styles/_tailwind.scss && prettier --write src/styles/_tailwind.scss",
    "test": "react-scripts test --modulePaths=src --coverage",
    "storybook": "start-storybook -p 6006"
  }
}
