{
  "name": "dashboard",
  "description": "Personal dashboard",
  "version": "1.1.1",
  "author": "Darek Kay <hello@darekkay.com> (https://darekkay.com/)",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "i18next": "19.0.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-grid-layout": "0.17.1",
    "react-i18next": "11.2.5",
    "react-modal": "3.11.1",
    "react-redux": "7.1.3",
    "react-scripts": "3.3.0",
    "reakit": "1.0.0-beta.13",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "redux-sagas-injector": "1.1.1",
    "redux-starter-kit": "0.8.1",
    "selectorator": "4.0.3"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@testing-library/react": "9.3.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.4",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/moment": "2.13.0",
    "@types/node": "12.12.16",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-grid-layout": "0.16.7",
    "@types/react-modal": "3.10.0",
    "@types/react-redux": "7.1.5",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "node-sass": "4.13.0",
    "plop": "2.5.3",
    "prettier": "1.19.1",
    "storybook-addon-themes": "5.2.0",
    "tailwindcss": "1.1.4",
    "ts-node": "^8.5.4",
    "typescript": "3.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://dashboard.darekkay.com",
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 75,
        "functions": 70,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/__stories__/**",
      "!**/react-app-env.d.ts",
      "!src/components/**/selectors.ts",
      "!src/widgets/**/properties.ts",
      "!src/widgets/**/configuration.tsx"
    ]
  },
  "license": "MIT",
  "main": "src/index.tsx",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/dashboard.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "ci": "npm run lint:check && npm run test",
    "format": "prettier --write -- \"{src,public,docs}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint:check": "tsc && eslint src/**/*.{ts,tsx,js}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,js}",
    "scan-widgets": "ts-node -P internals/scripts/tsconfig.json internals/scripts/scan-widgets.ts && prettier --write src/widgets/list.ts",
    "start": "react-scripts start",
    "tailwind": "tailwind build internals/tailwind/tailwind.css -c internals/tailwind/config.js --no-autoprefixer -o src/styles/_tailwind.scss && prettier --write src/styles/_tailwind.scss",
    "test": "react-scripts test --modulePaths=src --coverage",
    "storybook": "start-storybook -p 6006"
  }
}
